<%- include(`partials/header`)%>

<h1>Home</h1>
<p><%= pageStartContent %></p>

<% posts.forEach(function(post){ %>
    <h3><%= post.newPostTitle %></h3>
    <p><%= post.newPostContent.substring(0,450) + "... " %></p>

    <a href="posts/<%=post.newPostTitle%>">Read More</a>
<% }) %>

<%- include(`partials/footer`)%>
